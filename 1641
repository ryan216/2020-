class Solution(object):
    def countVowelStrings(self, n):
        """
        :type n: int
        :rtype: int
        """
        a,e,i,o,u=1,1,1,1,1
        for j in range(1,n):
            a=a+e+i+o+u
            e=e+i+o+u
            i=i+o+u
            o=o+u
        #return (n+1)*(n+2)*(n+3)*(n+4)/ 24
        return a+e+i+o+u
