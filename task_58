//58
class Solution {
    public int lengthOfLastWord(String s) 
    {
        int length = 0;
        for (int i = s.length() - 1; i >= 0; i--) 
        {
            if (s.charAt(i) != ' ') 
            {
                length++;
            } 
            else if (length != 0)   //避免末尾是空字符
            {
                return length;
            }
        }
        return length;
    }
}
