class Solution(object):
    def countVowelStrings(self, n):
        """
        :type n: int
        :rtype: int
        """
        a,e,i,o,u=1,1,1,1,1
        for j in range(1,n):
            a=a+e+i+o+u
            e=e+i+o+u
            i=i+o+u
            o=o+u
        #return (n+1)*(n+2)*(n+3)*(n+4)/ 24         4隔板，n+4个插入点（n+1个实际点，三个虚拟点，分别为，2个重叠，3个重叠，4个重叠一起）
        return a+e+i+o+u
